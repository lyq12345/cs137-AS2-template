{
  "name": "cs122b-fe-temp",
  "version": "1.0.0",
  "private": true,
  "description": "umi3项目模板",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build:test": "cross-env BUILD_PRODUCTION=test umi build",
    "build:uat": "cross-env BUILD_PRODUCTION=uat umi build",
    "build:prod": "cross-env BUILD_PRODUCTION=prod umi build",
    "deploy": "npm run site && npm run gh-pages",
    "fetch:blocks": "pro fetch-blocks --branch=umi@3 && npm run prettier",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier --check \"**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"**/*\"",
    "start": "npm run start:dev",
    "dev": "npm run start:dev",
    "serve": "npm run start:dev",
    "start:dev": "cross-env BUILD_PRODUCTION=dev MOCK=none umi dev",
    "start:no-mock": "cross-env BUILD_PRODUCTION=dev MOCK=none umi dev",
    "start:no-ui": "cross-env BUILD_PRODUCTION=dev UMI_UI=none umi dev",
    "start:test": "cross-env BUILD_PRODUCTION=test MOCK=none umi dev",
    "start:uat": "cross-env BUILD_PRODUCTION=uat MOCK=none umi dev",
    "test": "umi test",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components",
    "tsc": "tsc"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.0.0",
    "@ant-design/pro-layout": "5.0.8",
    "@ant-design/pro-table": "^2.0.0",
    "@sentry/react": "^5.27.6",
    "@sentry/tracing": "^5.27.6",
    "@umijs/hooks": "^1.8.0",
    "antd": "^4.8.0",
    "axios": "^0.21.1",
    "classnames": "^2.2.6",
    "hash.js": "^1.1.7",
    "history": "^4.10.1",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.11",
    "memoize-one": "^5.1.1",
    "moment": "^2.24.0",
    "omit.js": "^1.0.2",
    "path-to-regexp": "^2.4.0",
    "qs": "^6.9.3",
    "rc-resize-observer": "^0.2.1",
    "react": "^16.8.6",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^16.8.6",
    "react-helmet-async": "^1.0.4",
    "react-router-dom": "^5.1.2",
    "react-use": "^13.27.0",
    "redux-persist": "^6.0.0",
    "umi": "^3.2.14",
    "umi-request": "^1.0.8",
    "unstated-next": "^1.1.0",
    "use-json-comparison": "^1.0.5",
    "use-media-antd-query": "^1.0.1",
    "use-merge-value": "^1.0.1",
    "warning": "^4.0.3"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^1.0.18",
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-angular": "^9.1.1",
    "@types/classnames": "^2.2.7",
    "@types/express": "^4.17.0",
    "@types/history": "^4.7.2",
    "@types/jest": "^25.1.0",
    "@types/lodash": "^4.14.144",
    "@types/memoize-one": "^5.1.2",
    "@types/qs": "^6.9.1",
    "@types/react": "^16.9.17",
    "@types/react-copy-to-clipboard": "^4.3.0",
    "@types/react-dom": "^16.9.6",
    "@types/react-helmet": "^5.0.13",
    "@types/react-router-dom": "^5.1.4",
    "@types/warning": "^3.0.0",
    "@umijs/fabric": "2.0.5",
    "@umijs/plugin-blocks": "^2.0.5",
    "@umijs/preset-ant-design-pro": "^1.0.1",
    "@umijs/preset-react": "^1.3.0",
    "@umijs/preset-ui": "^2.0.9",
    "archiver": "^4.0.1",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "chalk": "^3.0.0",
    "compression-webpack-plugin": "^3.1.0",
    "cross-env": "^7.0.0",
    "cross-port-killer": "^1.1.1",
    "enzyme": "^3.11.0",
    "express": "^4.17.1",
    "gh-pages": "^2.0.1",
    "husky": "^4.0.7",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^10.0.0",
    "mockjs": "^1.0.1-beta3",
    "prettier": "^2.0.1",
    "pro-download": "1.0.1",
    "stylelint": "^13.0.0",
    "webpack-zip-plugin": "^0.1.6"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.ts*",
    "src/**/*.less",
    "config/**/*.js*",
    "scripts/**/*.js"
  ]
}
